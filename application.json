{
    "security": {
        "jwtIssuer": "login.binggl.net",
        "jwtSecret": "secret",
        "cookieName": "login_token",
        "loginRedirect": "https://login.binggl.net/auth/flow?~site=bookmarks&~url=http://localhost:3000",
        "claim": {
            "name": "mydms",
            "url": "http://localhost:3000",
            "roles": ["User", "Admin"]
        },
        "cacheDuration": "10m"
    },
    "database": {
        "connectionString": "mydms:mydms@localhost:3306/mydms",
        "dialect": "mysql"
    },
    "logging": {
        "logPrefix": "mydms-api",
        "rollingFileLogger": {
            "filePath": "/_logs/bookmars-api.log",
            "maxFileSize": 10,
            "numberOfMaxBackups": 4,
            "maxAge": 7,
            "compressFile": false
        }
    },
    "fileServer": {
	    "path": "ui",
	    "urlPath": "/ui"
    }
}
